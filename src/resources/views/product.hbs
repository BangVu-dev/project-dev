<div class="mt-4">
    <div class="row">

        {{#each products}}
        <div class="col-sm-6 col-lg-4">
            <div class="card card-course-item">                
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <h5 class="card-title">{{this.branch}}</h5>                  
                </div>
            </div>
        </div>
        {{/each}}

    </div>
</div>